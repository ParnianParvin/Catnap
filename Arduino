
int sensorPin1 = A0;
int sensorValue1;

int sensorPin2 = A2;
int sensorValue2;

void setup() 
{
Serial.begin(9600);

}

void loop() {
  
sensorValue1 = analogRead(sensorPin1);
sensorValue2 = analogRead(sensorPin2);

Serial.print(sensorValue1);
Serial.print(".");
Serial.println(sensorValue2);

delay(500);

}
